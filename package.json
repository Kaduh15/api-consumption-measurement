{
  "name": "api-consumption-measurement",
  "version": "1.0.0",
  "description": "API to manage water and gas consumption readings.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx watch src/server.ts",
    "dev:env": "tsx watch --env-file .env src/server.ts",
    "postinstall": "prisma generate",
    "start": "node .",
    "start:env": "node --env-file .env .",
    "lint": "eslint --ext .ts src/ --fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "compose:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "compose:down": "docker-compose -f docker-compose.yml down",
    "compose:up:dev": "docker-compose -f docker-compose.dev.yml up -d --build",
    "compose:up": "docker-compose -f docker-compose.yml up -d --build",
    "test": "vitest dev",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest dev --coverage"
  },
  "keywords": [],
  "author": "kaduh15",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaduh15/api-consumption-measurement.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "8",
    "eslint-plugin-import-helpers": "^1.3.1",
    "prisma": "^5.19.0",
    "sinon": "^18.0.0",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.0",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "@prisma/client": "5.19.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  }
}